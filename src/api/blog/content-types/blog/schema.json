{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "media_url": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "author": {
      "type": "string"
    },
    "author_profile_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "blog_contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-content.blog-content",
      "mappedBy": "blog"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "blog",
        "news",
        "case-study",
        "coaching-tips"
      ]
    },
    "views": {
      "type": "biginteger"
    },
    "sub_title": {
      "type": "richtext"
    },
    "service_type": {
      "type": "string"
    },
    "sub_service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sub-service.sub-service"
    },
    "toBeFaetured": {
      "type": "boolean",
      "default": false
    }
  }
}
